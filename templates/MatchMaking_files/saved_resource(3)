function validateEmailOrMobile(emailOrMobile){var mobileRegex=/^((\+)?[0-9]*(-)?)?[0-9]{7,}$/i;return validateEmail(emailOrMobile)||mobileRegex.test(emailOrMobile);}
function validateEmail(email){var regex=/^([A-Za-z0-9._%+-]+)@((?:[-a-z0-9]+\.)+[a-z]{2,})$/i;return regex.test(email.trim());}
function validateMobile(mobile){var str=$.trim(mobile);str=str.replace(/^0+/,'');if(str.indexOf('-')>-1)
{var result=str.split("-");result[1]=result[1].replace(/^0+/,'');str=result.join("-");}
if(str.indexOf('+')>-1)
{var result=str.split("+");result[1]=result[1].replace(/^0+/,'');str=result.join("+");}
var re=/^((\+)?[0-9]*(-)?)?[0-9]{7,}$/i;var isd='';var phone='';var data=new Array();if(re.test(str))
{str=str.split('+').join('');if(str.indexOf('-')>-1)
{isd=str.slice(0,str.indexOf('-'));phone=str.slice(str.indexOf('-')+1,str.length);}
else
{isd='';phone=str;}
data['flag']=1;data['phone']=phone;data['isd']=isd;}
else
{data['flag']=0;}
return data;}
